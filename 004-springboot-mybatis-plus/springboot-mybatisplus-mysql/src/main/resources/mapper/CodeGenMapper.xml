<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.keqi.springbootmybatisplusmysql.mapper.CodeGenMapper">

    <resultMap id="CodeGenMap" type="com.keqi.springbootmybatisplusmysql.domain.CodeGenDO">
        <id column="id" property="id"/>
        <result column="username" property="username"/>
        <result column="age" property="age"/>
        <result column="weight" property="weight"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="create_by" property="createBy"/>
        <result column="update_by" property="updateBy"/>
    </resultMap>

    <select id="selectByUsername" resultMap="CodeGenMap">
        select *
        from t_code_gen
        where username = #{username}
    </select>


</mapper>